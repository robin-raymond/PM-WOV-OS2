DEFINECITEM(CPointer)
COPYCITEM(CPointer)
MATCHCITEM(CPointer)

CPointer &CPointer::operator=(const CPointer &NewItem)
{
	m_Value = NewItem.m_Value;
	return *this;
}

BOOL CPointer::operator==(const CPointer &NewItem)
{
	return m_Value == NewItem.m_Value;
}


